{
  "name": "@org/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "api:dev": "nx serve @org/api",
    "api:build": "nx build @org/api",
    "api:lint": "nx lint @org/api",
    "web:dev": "nx dev @org/web",
    "web:build": "nx build @org/web",
    "web:lint": "nx lint @org/web",
    "dev:all": "nx run-many -t dev",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down",
    "infra": "docker compose up -d --build",
    "nx": "nx",
    "commit": "cz",
    "release": "semantic-release",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate --config=packages/database/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=packages/database/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=packages/database/drizzle.config.ts",
    "test:api": "nx test @org/api",
    "test:web": "nx test @org/web",
    "test:db": "nx run database:test",
    "test:e2e": "npx nx run @org/web-e2e:e2e",
    "test:all": "nx run-many -t test e2e"
  },
  "lint-staged": {
    "*.{js,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@next/eslint-plugin-next": "^16.0.1",
    "@nx/devkit": "22.4.2",
    "@nx/eslint": "22.4.2",
    "@nx/eslint-plugin": "22.4.2",
    "@nx/js": "22.4.2",
    "@nx/nest": "^22.4.2",
    "@nx/next": "^22.4.2",
    "@nx/node": "22.4.2",
    "@nx/playwright": "22.4.2",
    "@nx/vitest": "22.4.2",
    "@nx/web": "22.4.2",
    "@nx/webpack": "22.4.2",
    "@playwright/test": "^1.36.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "10.4.13",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.8.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.7",
    "nx": "22.4.2",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "semantic-release": "^25.0.3",
    "supertest": "^7.2.2",
    "tailwindcss": "3.4.3",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "unplugin-swc": "^1.5.9",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.8",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@testing-library/react": "^16.3.2",
    "axios": "^1.6.0",
    "drizzle-orm": "^0.45.1",
    "next": "~16.0.1",
    "postgres": "^3.4.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  }
}
